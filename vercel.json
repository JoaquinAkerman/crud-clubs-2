{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/clubs/.*",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
